{
    "name": "react-redux-typescript",
    "version": "1.0.0",
    "description": "Boilerplate for React, Redux, TypeScript",
    "main": "src/index.tsx",
    "type-check": "tsc",
    "scripts": {
        "tslint": "tslint -c tslint.json 'src/**/*.tsx'",
        "test": "jest",
        "test-watch": "npm test -- --watch",
        "test-coverage": "jest --coverage",
        "test-coverage-watch": "npm run test -- --watch --coverage",
        "dev": "webpack-dev-server --config webpack/config.dev.babel.js",
        "build:dev": "webpack --config webpack/config.dev.babel.js",
        "build:stage": "webpack --config webpack/config.stage.babel.js",
        "build:prod": "webpack --config webpack/config.prod.babel.js"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "keywords": [
        "react",
        "redux",
        "typescript"
    ],
    "author": "zyzdrian",
    "license": "ISC",
    "homepage": "",
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "7.1.0",
        "@babel/plugin-proposal-decorators": "7.1.2",
        "@babel/plugin-proposal-export-default-from": "7.2.0",
        "@babel/plugin-proposal-export-namespace-from": "7.0.0",
        "@babel/plugin-proposal-function-bind": "7.2.0",
        "@babel/plugin-proposal-function-sent": "7.1.0",
        "@babel/plugin-proposal-json-strings": "7.0.0",
        "@babel/plugin-proposal-numeric-separator": "7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "7.0.0",
        "@babel/plugin-proposal-optional-chaining": "7.2.0",
        "@babel/plugin-proposal-throw-expressions": "7.0.0",
        "@babel/plugin-syntax-dynamic-import": "7.0.0",
        "@babel/plugin-syntax-import-meta": "7.0.0",
        "@babel/plugin-transform-modules-commonjs": "7.1.0",
        "@babel/polyfill": "7.0.0",
        "@babel/preset-env": "7.1.0",
        "@babel/preset-react": "7.0.0",
        "@babel/preset-typescript": "7.1.0",
        "@babel/register": "7.0.0",
        "@types/enzyme": "3.1.15",
        "@types/enzyme-adapter-react-16": "1.0.3",
        "@types/jest": "23.3.10",
        "@types/react": "16.7.11",
        "@types/react-dom": "16.0.11",
        "@types/react-redux": "6.0.10",
        "@types/react-router-dom": "4.3.1",
        "@types/redux-form": "^7.4.12",
        "@types/webpack-env": "1.13.6",
        "autoprefixer": "9.4.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.4",
        "babel-plugin-jsx-control-statements": "4.0.0",
        "babel-plugin-syntax-dynamic-import": "6.18.0",
        "compression-webpack-plugin": "2.0.0",
        "css-loader": "1.0.1",
        "dotenv-webpack": "1.5.7",
        "enzyme": "3.7.0",
        "enzyme-adapter-react-16": "1.7.0",
        "favicons-webpack-plugin": "0.0.9",
        "file-loader": "2.0.0",
        "html-webpack-plugin": "3.2.0",
        "husky": "1.2.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "23.6.0",
        "node-sass": "4.10.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "prettier": "1.15.3",
        "pretty-quick": "1.8.0",
        "react-svg-loader": "2.1.0",
        "redux-devtools-extension": "2.13.7",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "ts-jest": "23.10.5",
        "tslint": "5.11.0",
        "tslint-config-prettier": "1.17.0",
        "tslint-plugin-prettier": "2.0.1",
        "tslint-react": "3.6.0",
        "typescript": "3.2.1",
        "uglifyjs-webpack-plugin": "2.0.1",
        "url-loader": "1.1.2",
        "webpack": "4.27.1",
        "webpack-bundle-analyzer": "3.0.3",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.14"
    },
    "dependencies": {
        "axios": "0.18.0",
        "classnames": "2.2.6",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "react-helmet": "5.2.0",
        "react-redux": "5.1.1",
        "react-router-dom": "4.3.1",
        "react-transition-group": "2.5.0",
        "redux": "4.0.1",
        "redux-form": "7.4.2",
        "redux-saga": "0.16.2",
        "redux-thunk": "2.3.0",
        "typesafe-actions": "2.0.4"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setupTests.ts",
        "collectCoverageFrom": [
            "src/**/*.ts?(x)",
            "!src/index.tsx"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
            "\\.(css|scss)$": "identity-obj-proxy"
        },
        "transform": {
            "^.+\\.tsx$": "babel-jest",
            "^.+\\.ts$": "babel-jest"
        },
        "testMatch": [
            "__tests__/**/*.ts?(x)",
            "**/?(*.)+(spec|test).ts?(x)"
        ]
    }
}
